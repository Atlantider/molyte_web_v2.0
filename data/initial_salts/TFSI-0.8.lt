    dihedral_coeff @dihedral:S-N-S-O 0.0    0.0   -0.004 0.0
    dihedral_coeff @dihedral:F-C-S-N 0.0    0.0    0.316 0.0
    dihedral_coeff @dihedral:S-N-S-C 7.833 -2.490 -0.764 0.0

  }
write_once("In Charges"){
    set type @atom:S  charge 0.816
    set type @atom:O  charge -0.424
    set type @atom:C  charge 0.28
    set type @atom:N  charge -0.528
    set type @atom:F  charge -0.128
  } #(end of atom charges)

