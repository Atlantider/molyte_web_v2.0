TFSI {


  write_once("In Init") {
    atom_style full
    units        real
    pair_style   lj/cut/coul/long 10.0
    bond_style   harmonic
    angle_style  harmonic
    dihedral_style opls
    improper_style cvff
    kspace_style pppm 0.0001
    pair_modify  mix arithmetic
  }

write_once("Data Masses") {
    @atom:S 32.060
    @atom:O 15.999
    @atom:C 12.011
    @atom:N 14.007
    @atom:F 18.998
  }
  write_once("In Settings") {
    pair_coeff @atom:S @atom:S 0.250 3.55
    pair_coeff @atom:O @atom:O 0.210 2.96
    pair_coeff @atom:C @atom:C 0.066 3.50
    pair_coeff @atom:N @atom:N 0.170 3.25
